{
  "expo": {
    "name": "WoofWalk",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/dog.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "react-native-auth0",
        {
          "domain": "dev-h5zqtrdr8n7sgz84.us.auth0.com"
        }
      ],
      ["expo-secure-store"],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow WoofWalk to access your location."
        }
      ]
    ],
    "splash": {
      "image": "./assets/dog.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.frontend"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/dog.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.frontend"
    },
    "web": {
      "favicon": "./assets/dog.png"
    },
    "extra": {
      "eas": {
        "projectId": "033aae3e-d033-461f-918c-ba45385d4e3a"
      }
    }
  }
}
